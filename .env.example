# LLM API Keys
# Get your API keys from:
# - Anthropic: https://console.anthropic.com/
# - OpenAI: https://platform.openai.com/api-keys
ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...

# Application Settings
# Ports for the analytics agent interface
UI_PORT=3000
GATEWAY_PORT=8080

# Logging
# Options: debug, info, warn, error
LOG_LEVEL=info

# Feature Flags
# Enable LLM enrichment for warehouse profiling
ENABLE_LLM_ENRICHMENT=true

# Warehouse Configuration
# Optional: Override default connection timeouts (milliseconds)
# WAREHOUSE_CONNECTION_TIMEOUT=30000
# WAREHOUSE_QUERY_TIMEOUT=60000

# Profiling Configuration
# Optional: Override profiling defaults
# PROFILING_MAX_SAMPLE_SIZE=1000
# PROFILING_TIMEOUT_SECONDS=60
# PROFILING_INCLUDE_ROW_COUNTS=true
