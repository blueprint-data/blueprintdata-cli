  },
  "dependencies": {
    "@blueprintdata/models": "workspace:*",
    "@blueprintdata/config": "workspace:*",
    "@google-cloud/bigquery": "^7.0.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
