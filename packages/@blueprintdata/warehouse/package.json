{
  "name": "@blueprintdata/warehouse",
  "version": "1.0.0",
  "description": "Warehouse connectors for BlueprintData (BigQuery, Postgres)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
